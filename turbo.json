{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"cache": true,
			"inputs": ["src/**", "package.json"],
			"outputs": ["dist/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"lint": {
			"cache": true,
			"dependsOn": ["^lint"]
		}
	},
	"globalEnv": ["NODE_ENV"]
}
